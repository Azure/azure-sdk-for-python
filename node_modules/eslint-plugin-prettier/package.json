  "peerDependencies": {
    "@types/eslint": ">=8.0.0",
    "eslint": ">=8.0.0",
    "prettier": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "@types/eslint": {
      "optional": true
    },
    "eslint-config-prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "prettier-linter-helpers": "^1.0.0",
