{
  "matrix": {
    "$IMPORT": "eng/pipelines/templates/stages/platform-matrix.json",
    "ArmTemplateParameters": "@{ enableAttestation = $true }"
  },
  "include": [
    {
      "Agent": {
        "ubuntu-20.04_ManagedHSM": {
